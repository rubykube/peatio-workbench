{
    "name": "barong",
    "oauth_endpoints": {
        "token": {
            "preserve_host": false,
            "listen_path": "/auth/token",
            "upstreams": {
                "balancing": "roundrobin",
                "targets": [
                    {
                        "target": "http://barong:8080/api/v1/sessions"
                    }
                ]
            },
            "strip_path": true,
            "append_path": false,
            "methods": [
                "GET",
                "POST"
            ]
        }
    },
    "token_strategy": {
        "name": "jwt",
        "settings": [
            {
                "alg": "RS256",
                "key": "secret"
            }
        ]
    }
}